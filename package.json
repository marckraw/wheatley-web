{
  "name": "bystro-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "lint:eslint": "eslint --fix",
    "build:analyze": "ANALYZE=true yarn build",
    "clean:all": "rm -rf .next && rm -rf ./out",
    "clean:run:dev": "yarn clean:all && yarn dev",
    "serve:export:production:analyze": "ANALYZE=true yarn serve:export:production",
    "serve:export:production": "yarn clean:all && yarn build && yarn export && npx http-serve ./out -p 8888",
    "kill:next": "npx kill-port 3000",
    "storybook": "start-storybook -p 6006",
    "storybook:without-cache-manager": "start-storybook -p 6006 --no-manager-cache",
    "storybook-docs": "start-storybook --docs --no-manager-cache",
    "build-storybook": "build-storybook",
    "build-storybook-docs": "build-storybook --docs",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "yarn build && start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "yarn build && start-server-and-test start http://localhost:3000 cypress:headless",
    "test": "jest --watch",
    "test:ci": "jest --ci __tests__",
    "prepare": "husky install"
  },
  "dependencies": {
    "@just-js/just": "^1.1.0-rc.1",
    "add": "^2.0.6",
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "next": "^12.0.8",
    "pollen-css": "^4.2.0",
    "react": "^17.0.2",
    "react-animate-height": "^2.0.23",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.24.0",
    "react-modal": "^3.14.3",
    "react-select": "^5.2.2",
    "react-table": "^7.7.0",
    "react-toastify": "^8.0.3",
    "sass": "^1.48.0",
    "swr": "^1.1.2",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@netlify/plugin-nextjs": "^4.1.3",
    "@next/bundle-analyzer": "^12.0.8",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.13",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/lodash": "^4.14.176",
    "@types/react": "^17.0.38",
    "@types/react-modal": "^3.13.1",
    "@types/react-table": "^7.7.7",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "commitizen": "^4.2.4",
    "css-loader": "5.2.6",
    "cypress": "^8.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.7.0",
    "eslint-config-next": "^12.0.8",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "netlify-cli": "^8.8.2",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "10.1.1",
    "start-server-and-test": "^1.14.0",
    "style-loader": "2.0.0",
    "typescript": "^4.5.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
